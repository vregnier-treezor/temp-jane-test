{
  "name": "trzconnect/client-antelop",
  "description": "Treezor PHP Client Antelop library",
  "type": "library",
  "license": "private",
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.5",
    "jane-php/open-api-runtime": "^7.3",
    "nyholm/psr7": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Sdk\\": "src/sdk"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "jane-php/open-api-3": "^7.3"
  }
}
